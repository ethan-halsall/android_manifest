<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- Github remote -->
  <remote  name="github"
           fetch="https://github.com/" />

  <!-- Gitlab remote -->
  <remote  name="gitlab"
           fetch="https://gitlab.com/" />

  <!-- DU remote -->
  <remote  name="du"
           revision="p9x"
           fetch="https://github.com/DirtyUnicorns/"
